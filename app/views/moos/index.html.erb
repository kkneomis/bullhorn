

<div class="container">
  <div class="text-center">
    <h1 style="font-weight:bold; color:orange;"> <%= @moos.count %> Moos mooed on Kakmoo </h1>
    <%= link_to 'New Moo', new_moo_path, class:"btn btn-success btn-lg" %>
    
  </div>
</div><br>
    <% @moos.each do |moo| %>
  
    <div class="panel panel-primary">
      <div class="panel-heading">
        <%= moo.content %> 
      </div>
      <div class="panel-footer"> sent by <%= moo.user.name %> the <%= moo.user.title %>  on   <%= moo.created_at.strftime("%b/%d/%Y") %> at   <%= moo.created_at.strftime("%I:%M%p") %></div>
    </div>
    <% end %>



<br>
